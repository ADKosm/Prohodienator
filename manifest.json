{
  "manifest_version": 2,

  "name": "Prohodienator",
  "description": "This extension helps spend time to useful things",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
      "20": "img/icon20.png",
      "128": "img/icon128.png"
    },
    "default_popup": "html/popup.html"
  },
  "icons": {
    "48": "img/icon48.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "contextMenus",
    "tabs",
    "webNavigation",
    "alarms"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }

}